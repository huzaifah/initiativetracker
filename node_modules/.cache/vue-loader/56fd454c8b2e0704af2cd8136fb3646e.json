: {\n          required: true\n        },\n        country: {\n          required: true\n        }\n      }\n    };\n  },\n  methods: {\n    getError(fieldName) {\n      return this.errors.first(fieldName);\n    },\n    validate() {\n      return this.$validator.validateAll().then(res => {\n        this.$emit(\"on-validated\", res);\n        return res;\n      });\n    }\n  },\n  watch: {\n    street() {\n      this.touched.street = true;\n    },\n    streetNo() {\n      this.touched.streetNo = true;\n    }\n  }\n};\n",{"version":3,"sources":["ThirdStep.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4GA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ThirdStep.vue","sourceRoot":"src/pages/Dashboard/Forms/Wizard","sourcesContent":["<template>\n  <div class=\"md-layout\">\n    <div class=\"md-layout-item md-size-100\">\n      <h5 class=\"info-text\">Are you living in a nice area?</h5>\n    </div>\n    <div class=\"md-layout-item md-size-70 md-small-size-100\">\n      <md-field\n        :class=\"[\n          { 'md-valid': !errors.has('street name') && touched.street },\n          { 'md-error': errors.has('street name') }\n        ]\"\n      >\n        <label>Street Name</label>\n        <md-input\n          v-model=\"street\"\n          data-vv-name=\"street name\"\n          type=\"text\"\n          name=\"street name\"\n          required\n          v-validate=\"modelValidations.street\"\n        >\n        </md-input>\n        <slide-y-down-transition>\n          <md-icon class=\"error\" v-show=\"errors.has('street name')\"\n            >close</md-icon\n          >\n        </slide-y-down-transition>\n        <slide-y-down-transition>\n          <md-icon\n            class=\"success\"\n            v-show=\"!errors.has('street name') && touched.street\"\n            >done</md-icon\n          >\n        </slide-y-down-transition>\n      </md-field>\n    </div>\n    <div class=\"md-layout-item md-size-30 md-small-size-100\">\n      <md-field\n        :class=\"[\n          { 'md-valid': !errors.has('street number') && touched.streetNo },\n          { 'md-error': errors.has('street number') }\n        ]\"\n      >\n        <label>Street Number</label>\n        <md-input\n          v-model=\"streetNo\"\n          data-vv-name=\"street number\"\n          type=\"text\"\n          name=\"street number\"\n          required\n          v-validate=\"modelValidations.streetNo\"\n        >\n        </md-input>\n        <slide-y-down-transition>\n          <md-icon class=\"error\" v-show=\"errors.has('street name')\"\n            >close</md-icon\n          >\n        </slide-y-down-transition>\n        <slide-y-down-transition>\n          <md-icon\n            class=\"success\"\n            v-show=\"!errors.has('street number') && touched.streetNo\"\n            >done</md-icon\n          >\n        </slide-y-down-transition>\n      </md-field>\n    </div>\n    <div class=\"md-layout-item md-small-size-100\">\n      <md-field\n        :class=\"[\n          { 'md-valid': !errors.has('city') && touched.city },\n          { 'md-error': errors.has('city') }\n        ]\"\n      >\n        <label>City</label>\n        <md-input\n          v-model=\"city\"\n          data-vv-name=\"city\"\n          type=\"text\"\n          name=\"city\"\n          required\n          v-validate=\"modelValidations.city\"\n        >\n        </md-input>\n        <slide-y-down-transition>\n          <md-icon class=\"error\" v-show=\"errors.has('city')\">close</md-icon>\n        </slide-y-down-transition>\n        <slide-y-down-transition>\n          <md-icon class=\"success\" v-show=\"!errors.has('city') && touched.city\"\n            >done</md-icon\n          >\n        </slide-y-down-transition>\n      </md-field>\n    </div>\n    <div class=\"md-layout-item md-small-size-100\">\n      <md-field>\n        <label for=\"select\">Single Select</label>\n        <md-select v-model=\"select\" name=\"select\">\n          <md-option value=\"angola\">Angola</md-option>\n          <md-option value=\"france\">France</md-option>\n          <md-option value=\"germany\">Germany</md-option>\n          <md-option value=\"england\">England</md-option>\n        </md-select>\n      </md-field>\n    </div>\n  </div>\n</template>\n<script>\nimport { SlideYDownTransition } from \"vue2-transitions\";\nexport default {\n  components: {\n    SlideYDownTransition\n  },\n  data() {\n    return {\n      street: \"\",\n      streetNo: \"\",\n      city: \"\",\n      country: \"\",\n      select: null,\n      touched: {\n        street: false\n      },\n      countryOptions: [\"One\", \"Two\", \"Three\", \"Four\", \"Five\", \"Six\"],\n      modelValidations: {\n        street: {\n          required: true,\n          min: 5\n        },\n        streetNo: {\n          required: true,\n          min: 5\n        },\n        city: {\n          required: true\n        },\n        country: {\n          required: true\n        }\n      }\n    };\n  },\n  methods: {\n    getError(fieldName) {\n      return this.errors.first(fieldName);\n    },\n    validate() {\n      return this.$validator.validateAll().then(res => {\n        this.$emit(\"on-validated\", res);\n        return res;\n      });\n    }\n  },\n  watch: {\n    street() {\n      this.touched.street = true;\n    },\n    streetNo() {\n      this.touched.streetNo = true;\n    }\n  }\n};\n</script>\n<style></style>\n"]}]}