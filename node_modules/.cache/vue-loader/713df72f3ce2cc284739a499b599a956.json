;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ProductCard.vue","sourceRoot":"src/components/Cards","sourcesContent":["<template>\n  <md-card\n    class=\"md-card-product\"\n    @mouseleave.native=\"onMouseLeave\"\n    :data-count=\"hoverCount\"\n  >\n    <md-card-header\n      @mouseenter.native=\"onMouseOver\"\n      :data-header-animation=\"headerAnimation\"\n      :class=\"[\n        { hovered: imgHovered },\n        { hinge: headerDown },\n        { fadeInDown: fixedHeader }\n      ]\"\n      class=\"md-card-header-image animated\"\n    >\n      <slot name=\"imageHeader\"></slot>\n    </md-card-header>\n\n    <md-card-content>\n      <div\n        class=\"md-card-action-buttons text-center\"\n        v-if=\"headerAnimation === 'true'\"\n      >\n        <md-button\n          class=\"md-danger md-simple fix-broken-card\"\n          @click=\"fixHeader\"\n          v-if=\"headerDown\"\n        >\n          <slot name=\"fixed-button\"></slot> Fix Header!\n        </md-button>\n        <md-button class=\"md-simple md-just-icon\">\n          <slot name=\"first-button\"></slot>\n        </md-button>\n        <md-button class=\"md-success md-simple md-just-icon\">\n          <slot name=\"second-button\"></slot>\n        </md-button>\n        <md-button class=\"md-danger md-simple md-just-icon\">\n          <slot name=\"third-button\"></slot>\n        </md-button>\n      </div>\n      <slot name=\"title\"></slot>\n      <slot name=\"description\"></slot>\n    </md-card-content>\n\n    <md-card-actions md-alignment=\"space-between\">\n      <slot name=\"footer\"></slot>\n    </md-card-actions>\n  </md-card>\n</template>\n\n<script>\nexport default {\n  name: \"product-cards\",\n  props: {\n    headerAnimation: {\n      type: String,\n      default: \"true\"\n    }\n  },\n  data() {\n    return {\n      hoverCount: 0,\n      imgHovered: false,\n      fixedHeader: false\n    };\n  },\n  computed: {\n    headerDown() {\n      return this.hoverCount > 15;\n    }\n  },\n  methods: {\n    headerBack: function() {\n      this.fixedHeader = false;\n    },\n    fixHeader: function() {\n      this.hoverCount = 0;\n      this.fixedHeader = true;\n\n      setTimeout(this.headerBack, 480);\n    },\n    onMouseOver: function() {\n      if (this.headerAnimation === \"true\") {\n        this.imgHovered = true;\n        this.hoverCount++;\n      }\n    },\n    onMouseLeave: function() {\n      if (this.headerAnimation === \"true\") {\n        this.imgHovered = false;\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\"></style>\n"]}]}